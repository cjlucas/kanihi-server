<h1>Listing tracks</h1>

<table>
  <tr>
    <th>Track name</th>
    <th>Subtitle</th>
    <th>Track num</th>
    <th>Track total</th>
    <th>Track artist</th>
    <th>Track artist sort order</th>
    <th>Album artist</th>
    <th>Album artist sort order</th>
    <th>Album name</th>
    <th>Genre</th>
    <th>Date</th>
    <th>Original date</th>
    <th>Disc num</th>
    <th>Disc total</th>
    <th>Disc subtitle</th>
    <th>Group</th>
    <th>Lyrics</th>
    <th>Composer</th>
    <th>Mood</th>
    <th>Compilation</th>
    <th>Comment</th>
    <th>Album art</th>
    <th>Duration</th>
    <th>Mtime</th>
    <th>Size</th>
    <th>Uri</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tracks.each do |track| %>
  <tr>
    <td><%= track.track_name %></td>
    <td><%= track.subtitle %></td>
    <td><%= track.track_num %></td>
    <td><%= track.track_total %></td>
    <td><%= track.track_artist %></td>
    <td><%= track.track_artist_sort_order %></td>
    <td><%= track.album_artist %></td>
    <td><%= track.album_artist_sort_order %></td>
    <td><%= track.album_name %></td>
    <td><%= track.genre %></td>
    <td><%= track.date %></td>
    <td><%= track.original_date %></td>
    <td><%= track.disc_num %></td>
    <td><%= track.disc_total %></td>
    <td><%= track.disc_subtitle %></td>
    <td><%= track.group %></td>
    <td><%= track.lyrics %></td>
    <td><%= track.composer %></td>
    <td><%= track.mood %></td>
    <td><%= track.compilation %></td>
    <td><%= track.comment %></td>
    <td><%= track.album_art_id %></td>
    <td><%= track.duration %></td>
    <td><%= track.mtime %></td>
    <td><%= track.size %></td>
    <td><%= track.uri %></td>
    <td><%= link_to 'Show', track %></td>
    <td><%= link_to 'Edit', edit_track_path(track) %></td>
    <td><%= link_to 'Destroy', track, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

